<div class="row panel-content panel-post-item">
    <%
        var title = !!page.title ? page.title : 'No Title';
        var link = !!page.link ? page.link : url_for(page.path);
    %>

    <div class="article-wrap">
        <%- partial('./article-content',
                    {'title': title,
                        'link': link,
                        'content': page.content,
                        'moreBtn': false}) %>
        <%- partial('./article-meta', {'article': page}) %>
    </div>

    <div class="panel-post-nav">
        <div class="box-post-nav">
            <%- partial('_widget/article-nav') %>
        </div>
    </div>
</div>
